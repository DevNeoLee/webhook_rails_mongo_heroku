<p id="notice"><%= notice %></p>

<h1>Shipment Locations</h1>

<table>
  <thead>
    <tr>
      <th>Shipment</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Brokeragejob</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipment_locations.each do |shipment_location| %>
      <tr>
        <td><%= shipment_location.shipment %></td>
        <td><%= shipment_location.latitude %></td>
        <td><%= shipment_location.longitude %></td>
        <td><%= shipment_location.BrokerageJob_id %></td>
        <td><%= link_to 'Show', shipment_location %></td>
        <td><%= link_to 'Edit', edit_shipment_location_path(shipment_location) %></td>
        <td><%= link_to 'Destroy', shipment_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipment Location', new_shipment_location_path %>
